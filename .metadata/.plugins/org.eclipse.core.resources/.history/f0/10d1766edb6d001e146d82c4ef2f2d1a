package Day11;

import java.util.Scanner;

public class JE {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int i;
		while(true) {
			System.out.println("숫자를 압력");
			i = sc.nextInt();
			if(i==0) {
				System.out.println("end");
				break;
			}
		}
		System.out.println(i);
	}

}
